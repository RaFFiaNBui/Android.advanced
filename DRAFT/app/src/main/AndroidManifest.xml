<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ablesson1">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name="com.example.ablesson1.history.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.ablesson1.DevelopersActivity" />
        <activity android:name="com.example.ablesson1.BaseActivity" />
        <activity android:name="com.example.ablesson1.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--<activity android:name=".SecondActivity" />--><!--После перехода на одну активити, данная активити не используется-->
        <activity android:name="com.example.ablesson1.SettingsActivity" />
    </application>

</manifest>